# === AI Agent ===
LLM_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_MODEL=
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=
OPENROUTER_API_KEY=
OPENROUTER_MODEL=
JUDGE_MODEL=

# === JWT Secret ===
SECRET_KEY=  # Generate: openssl rand -hex 32

# === Telemetry ===
# Optional local JSONL file for quick analysis
TELEMETRY_FILE=

# === Traefik Dashboard ===
# Generate with: htpasswd -nb admin yourpassword
# Example: admin:$apr1$xyz$hash
TRAEFIK_DASHBOARD_AUTH=

# === Flower Monitoring ===
FLOWER_USER=admin
FLOWER_PASSWORD=  # Generate: openssl rand -base64 16
